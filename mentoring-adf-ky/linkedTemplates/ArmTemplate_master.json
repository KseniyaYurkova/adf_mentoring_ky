{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mentoring-adf-ky"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_ADSL_SinkOnline_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADSL_SinkOnline'"
		},
		"LS_ABLB_azurementoringauky_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://azurementoringauky.blob.core.windows.net/"
		},
		"LS_ABLB_azurementoringstorageky_properties_typeProperties_sasUri_secretName": {
			"type": "string",
			"defaultValue": "sasazurementoringstorageky"
		},
		"LS_ADLS_azurementoringdatalakeky_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://azurementoringdatalakeky.blob.core.windows.net"
		},
		"LS_ADSL_SinkOnline_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurementoringdatalakeky.dfs.core.windows.net/"
		},
		"LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnectdatamart"
		},
		"LS_ASQL_azurementoringky_mssql_movies_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlconnectionstringazurementoringky"
		},
		"LS_KV_MentoringKeyVaultKY_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mentoring-key-vault-ky.vault.azure.net/"
		},
		"LS_MSQL_CinemaTickets_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "mssqlonpremserverconnection"
		},
		"SL_ABLB_IncCopyFileOnline_properties_typeProperties_sasUri_secretName": {
			"type": "string",
			"defaultValue": "sasonlineinccopy"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mentoring-adf-ky_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_ADSL_SinkOnline_accountKey": {
						"value": "[parameters('LS_ADSL_SinkOnline_accountKey')]"
					},
					"LS_ABLB_azurementoringauky_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('LS_ABLB_azurementoringauky_properties_typeProperties_serviceEndpoint')]"
					},
					"LS_ADLS_azurementoringdatalakeky_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('LS_ADLS_azurementoringdatalakeky_properties_typeProperties_serviceEndpoint')]"
					},
					"LS_ADSL_SinkOnline_properties_typeProperties_url": {
						"value": "[parameters('LS_ADSL_SinkOnline_properties_typeProperties_url')]"
					},
					"LS_KV_MentoringKeyVaultKY_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KV_MentoringKeyVaultKY_properties_typeProperties_baseUrl')]"
					},
					"LS_ABLB_azurementoringstorageky_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('LS_ABLB_azurementoringstorageky_properties_typeProperties_sasUri_secretName')]"
					},
					"LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_DataMart_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mentoring-adf-ky_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ASQL_azurementoringky_mssql_movies_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_azurementoringky_mssql_movies_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_MSQL_CinemaTickets_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_MSQL_CinemaTickets_properties_typeProperties_connectionString_secretName')]"
					},
					"SL_ABLB_IncCopyFileOnline_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('SL_ABLB_IncCopyFileOnline_properties_typeProperties_sasUri_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ky_ArmTemplate_0')]"
			]
		},
		{
			"name": "mentoring-adf-ky_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ky_ArmTemplate_1')]"
			]
		},
		{
			"name": "mentoring-adf-ky_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mentoring-adf-ky_ArmTemplate_2')]"
			]
		}
	]
}